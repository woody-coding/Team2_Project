[1mdiff --git a/src/main/webapp/WEB-INF/views/ticket/ticketDetailpage/datepicker.js b/src/main/webapp/WEB-INF/views/ticket/ticketDetailpage/datepicker.js[m
[1mnew file mode 100644[m
[1mindex 0000000..5105f08[m
[1m--- /dev/null[m
[1m+++ b/src/main/webapp/WEB-INF/views/ticket/ticketDetailpage/datepicker.js[m
[36m@@ -0,0 +1,68 @@[m
[32m+[m
[32m+[m
[32m+[m[32mdocument.addEventListener('DOMContentLoaded', () => {[m
[32m+[m[32m    const calendarDays = document.getElementById('calendarDays');[m
[32m+[m[32m    const monthYear = document.getElementById('monthYear');[m
[32m+[m[32m    const prevMonth = document.getElementById('prevMonth');[m
[32m+[m[32m    const nextMonth = document.getElementById('nextMonth');[m
[32m+[m
[32m+[m[32m    const today = new Date();[m
[32m+[m[32m    let currentMonth = today.getMonth();[m
[32m+[m[32m    let currentYear = today.getFullYear();[m
[32m+[m
[32m+[m[32m    // Helper function: Generate days for the calendar[m
[32m+[m[32m    function generateCalendar(year, month) {[m
[32m+[m[32m        calendarDays.innerHTML = ''; // Clear existing days[m
[32m+[m
[32m+[m[32m        const firstDay = new Date(year, month, 1);[m
[32m+[m[32m        const lastDay = new Date(year, month + 1, 0);[m
[32m+[m
[32m+[m[32m        const firstDayIndex = firstDay.getDay();[m
[32m+[m[32m        const daysInMonth = lastDay.getDate();[m
[32m+[m
[32m+[m[32m        // Add blank spaces for days before the 1st of the month[m
[32m+[m[32m        for (let i = 0; i < firstDayIndex; i++) {[m
[32m+[m[32m            const emptyCell = document.createElement('div');[m
[32m+[m[32m            calendarDays.appendChild(emptyCell);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Add days of the month[m
[32m+[m[32m        for (let day = 1; day <= daysInMonth; day++) {[m
[32m+[m[32m            const dayCell = document.createElement('div');[m
[32m+[m[32m            dayCell.textContent = day;[m
[32m+[m[32m            dayCell.addEventListener('click', () => {[m
[32m+[m[32m                // Remove existing selected class[m
[32m+[m[32m                document.querySelectorAll('.selected').forEach(el => el.classList.remove('selected'));[m
[32m+[m[32m                dayCell.classList.add('selected');[m
[32m+[m[41m				[m
[32m+[m				[32m//ÎÇ†ÏßúÌôïÏù∏ -> Ìï¥Îãπ ÎÇ†ÏßúÎ•º Ïè¥Ï§òÏÑú Îß§Ïπ≠[m
[32m+[m[32m                alert(`Selected Date: ${year}-${month + 1}-${day}`);[m
[32m+[m[32m            });[m
[32m+[m[32m            calendarDays.appendChild(dayCell);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        monthYear.textContent = `${year}ÎÖÑ ${month + 1}Ïõî`;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Event listeners for navigation[m
[32m+[m[32m    prevMonth.addEventListener('click', () => {[m
[32m+[m[32m        currentMonth--;[m
[32m+[m[32m        if (currentMonth < 0) {[m
[32m+[m[32m            currentMonth = 11;[m
[32m+[m[32m            currentYear--;[m
[32m+[m[32m        }[m
[32m+[m[32m        generateCalendar(currentYear, currentMonth);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    nextMonth.addEventListener('click', () => {[m
[32m+[m[32m        currentMonth++;[m
[32m+[m[32m        if (currentMonth > 11) {[m
[32m+[m[32m            currentMonth = 0;[m
[32m+[m[32m            currentYear++;[m
[32m+[m[32m        }[m
[32m+[m[32m        generateCalendar(currentYear, currentMonth);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Initial calendar generation[m
[32m+[m[32m    generateCalendar(currentYear, currentMonth);[m
[32m+[m[32m});[m
\ No newline at end of file[m
