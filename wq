[32m+[m[32mconst targetTime = new Date();[m
[32m+[m[32mtargetTime.setHours(16, 0, 0); // ex) Î≤ÑÌäº ÌôúÏÑ±Ìôî ÏãúÍ∞Ñ ÏÑ§Ï†ï (Ïãú, Î∂Ñ, Ï¥à)[m
[32m+[m
[32m+[m[32m// 'ÏòàÎß§ÌïòÍ∏∞' Î≤ÑÌäºÏóê Î™®Îì† Ìï®ÏàòÍ∞Ä Ï†ëÍ∑º Í∞ÄÎä•ÌïòÎèÑÎ°ù Ï†ÑÏó≠ Î≥ÄÏàòÎ°ú ÏÑ†Ïñ∏[m
[32m+[m[32mlet reserveButton;[m[41m [m
[32m+[m
[32m+[m[32mfunction updateButton() {[m
[32m+[m[32m    const now = new Date();[m
[32m+[m[32m    const timeDiff = targetTime - now;[m
[32m+[m
[32m+[m[32m    if (timeDiff <= 0) {[m
[32m+[m[32m        reserveButton.disabled = false;[m
[32m+[m[32m        reserveButton.textContent = 'ÏòàÎß§ÌïòÍ∏∞';[m
[32m+[m[32m    } else {[m
[32m+[m[32m        const hours = Math.floor(timeDiff / (1000 * 60 * 60));[m
[32m+[m[32m        const minutes = Math.floor((timeDiff % (1000 * 60 * 60)) / (1000 * 60));[m
[32m+[m[32m        const seconds = Math.floor((timeDiff % (1000 * 60)) / 1000);[m
[32m+[m
[32m+[m[32m        // ÏãúÍ∞Ñ, Î∂Ñ, Ï¥àÎ•º Í∞ÅÍ∞Å Îëê ÏûêÎ¶¨ Ïà´ÏûêÎ°ú ÎßåÎì§Ïñ¥ "00:00:00" ÌòïÏãùÏúºÎ°ú[m
[32m+[m[32m        reserveButton.textContent = `${hours.toString().padStart(2, '0')}:${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;[m
[32m+[m[41m        [m
[32m+[m[32m        // Îß§ Ï¥àÎßàÎã§ Ïã§ÏãúÍ∞ÑÏúºÎ°ú ÌôîÎ©¥Ïóê Î≥¥Ïó¨Ï£ºÍ∏∞ ÏúÑÌïú ÏÑ§Ï†ï[m
[32m+[m[32m        setTimeout(updateButton, 1000);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction startReservation() {[m
[32m+[m[32m    // Ïó¨Í∏∞Ïóê ÏòàÎß§ Ï†àÏ∞® Î°úÏßÅ ÎÑ£Í∏∞!![m
[32m+[m[32m    alert('ÏòàÎß§ ÏãúÏûë!');[m
[32m+[m[41m    [m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction init() {[m
[32m+[m[32m    // reserveButtonÏùÑ Ï¥àÍ∏∞Ìôî[m
[32m+[m[32m    reserveButton = document.querySelector('.bookTicket');[m[41m [m
[32m+[m[41m    [m
[32m+[m[32m    // ÌéòÏù¥ÏßÄ Î°úÎìú ÎêòÎ©¥ ÏûêÎèôÏúºÎ°ú ÌÉÄÏù¥Î®∏ Ïù¥Î≤§Ìä∏ Ìï®Ïàò ÏãúÏûë[m
[32m+[m[32m    updateButton();[m
[32m+[m
[32m+[m[32m    // Î≤ÑÌäºÏù¥ ÌôúÏÑ±ÌôîÎê† Îïå Ìïú Î≤àÎßå ÌÅ¥Î¶≠ Ïù¥Î≤§Ìä∏ Î¶¨Ïä§ÎÑà Ï∂îÍ∞Ä[m
[32m+[m[32m    reserveButton.addEventListener('click', startReservation);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mwindow.addEventListener('load', init);[m
